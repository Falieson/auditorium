<h1>Listing projects</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Git url</th>
      <th>Demo url</th>
      <th>Version</th>
      <th>Lastpublished</th>
      <th>Firstpublished</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td><%= project.title %></td>
        <td><%= project.description %></td>
        <td><%= project.git_url %></td>
        <td><%= project.demo_url %></td>
        <td><%= project.version %></td>
        <td><%= project.lastpublished %></td>
        <td><%= project.firstpublished %></td>
        <td><%= link_to 'Show', project %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>